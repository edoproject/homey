<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.edoproject.loco">
    android:versionCode="1"
    android:versionName="1.0"

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_house_heart"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_house_heart"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.SPLASH" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ChecklistActivity">
            <intent-filter>
                <action android:name="android.intent.action.INTRO" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".RecycleListActivity">
            <intent-filter>
                <action android:name="android.intent.action.INTRO" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>